{
  "name": "reachvarianttool",
  "displayName": "Reach Variant Tool",
  "description": "Support for the programming language used in Reach Variant Tool.",
  "icon": "icon.png",
  "version": "0.2.0",
  "license": "GPL-3.0-only",
  "author": "Unfunkable",
  "publisher": "Unfunkable",
  "repository": {
    "type": "git",
    "url": "https://github.com/Unfunkable/VSCode-ReachVariantTool"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:rvt"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.32.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "rvt",
        "aliases": [
          "ReachVariantTool",
          "rvt"
        ],
        "extensions": [
          ".rvt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rvt",
        "scopeName": "source.rvt",
        "path": "./syntaxes/rvt.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "rvt",
        "path": "./snippets.json"
      }
    ]
  }
}
